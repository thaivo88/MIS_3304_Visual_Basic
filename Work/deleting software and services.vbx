Sub RVC_5200_deleteSoftwareandServices()
'
' RVC_5200_deleteSoftwareandServices Macro
'

'
    ActiveSheet.Range("$A$1:$R$26446").AutoFilter Field:=16, Criteria1:= _
        "=Services", Operator:=xlOr, Criteria2:="="
    Rows("85:85").Select
    Range("E85").Activate
    Range(Selection, Selection.End(xlDown)).Select
    Selection.Delete Shift:=xlUp
End Sub

